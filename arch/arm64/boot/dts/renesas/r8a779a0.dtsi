#include <dt-bindings/clock/r8a779a0-cpg-mssr.h>
#include <dt-bindings/interrupt-controller/arm-gic.h>
#include <dt-bindings/power/r8a779a0-sysc.h>
#include <dt-bindings/ipmmu/rcar-ipmmu.h>

/ {
	compatible = "renesas,r8a779a0";
	#address-cells = <2>;
	#size-cells = <2>;

	aliases {
		i2c0 = &i2c0;
		i2c1 = &i2c1;
		i2c2 = &i2c2;
		i2c3 = &i2c3;
		i2c4 = &i2c4;
		i2c5 = &i2c5;
		i2c6 = &i2c6;
	};

	/* External CAN clock - to be overridden by boards that provide it */
	can_clk: can {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <0>;
	};

	cpus {
		#address-cells = <1>;
		#size-cells = <0>;

		cpu-map {
			cluster0 {
				core0 {
					cpu = <&a76_0>;
				};
				core1 {
					cpu = <&a76_1>;
				};
			};

			cluster1 {
				core0 {
					cpu = <&a76_2>;
				};
				core1 {
					cpu = <&a76_3>;
				};
			};

			cluster2 {
				core0 {
					cpu = <&a76_4>;
				};
				core1 {
					cpu = <&a76_5>;
				};
			};

			cluster3 {
				core0 {
					cpu = <&a76_6>;
				};
				core1 {
					cpu = <&a76_7>;
				};
			};
		};

		a76_0: cpu@0 {
			device_type = "cpu";
			compatible = "arm,cortex-a76", "arm,armv8";
			reg = <0>;
			clocks = <&cpg CPG_CORE R8A779A0_CLK_Z0>;
			power-domains = <&sysc R8A779A0_PD_CA76_CPU0>;
			enable-method = "psci";
			next-level-cache = <&L2_CA76>;
		};

		a76_1: cpu@100 {
			device_type = "cpu";
			compatible = "arm,cortex-a76", "arm,armv8";
			reg = <0x100>;
			clocks = <&cpg CPG_CORE R8A779A0_CLK_Z0>;
			power-domains = <&sysc R8A779A0_PD_CA76_CPU1>;
			enable-method = "psci";
			next-level-cache = <&L2_CA76>;
		};

		a76_2: cpu@10000 {
			device_type = "cpu";
			compatible = "arm,cortex-a76", "arm,armv8";
			reg = <0x10000>;
			clocks = <&cpg CPG_CORE R8A779A0_CLK_Z0>;
			power-domains = <&sysc R8A779A0_PD_CA76_CPU2>;
			enable-method = "psci";
			next-level-cache = <&L2_CA76>;
		};

		a76_3: cpu@10100 {
			device_type = "cpu";
			compatible = "arm,cortex-a76", "arm,armv8";
			reg = <0x10100>;
			clocks = <&cpg CPG_CORE R8A779A0_CLK_Z0>;
			power-domains = <&sysc R8A779A0_PD_CA76_CPU3>;
			enable-method = "psci";
			next-level-cache = <&L2_CA76>;
		};

		a76_4: cpu@20000 {
			device_type = "cpu";
			compatible = "arm,cortex-a76", "arm,armv8";
			reg = <0x20000>;
			clocks = <&cpg CPG_CORE R8A779A0_CLK_Z0>;
			power-domains = <&sysc R8A779A0_PD_CA76_CPU4>;
			enable-method = "psci";
			next-level-cache = <&L2_CA76>;
		};

		a76_5: cpu@20100 {
			device_type = "cpu";
			compatible = "arm,cortex-a76", "arm,armv8";
			reg = <0x20100>;
			clocks = <&cpg CPG_CORE R8A779A0_CLK_Z0>;
			power-domains = <&sysc R8A779A0_PD_CA76_CPU5>;
			enable-method = "psci";
			next-level-cache = <&L2_CA76>;
		};

		a76_6: cpu@30000 {
			device_type = "cpu";
			compatible = "arm,cortex-a76", "arm,armv8";
			reg = <0x30000>;
			clocks = <&cpg CPG_CORE R8A779A0_CLK_Z0>;
			power-domains = <&sysc R8A779A0_PD_CA76_CPU6>;
			enable-method = "psci";
			next-level-cache = <&L2_CA76>;
		};

		a76_7: cpu@30100 {
			device_type = "cpu";
			compatible = "arm,cortex-a76", "arm,armv8";
			reg = <0x30100>;
			clocks = <&cpg CPG_CORE R8A779A0_CLK_Z0>;
			power-domains = <&sysc R8A779A0_PD_CA76_CPU7>;
			enable-method = "psci";
			next-level-cache = <&L2_CA76>;
		};

		L2_CA76: cache-controller {
			compatible = "cache";
			power-domains = <&sysc R8A779A0_PD_CA76_CPU0>;
			cache-unified;
			cache-level = <2>;
		};
	};

	psci {
		compatible = "arm,psci-0.2";
		method = "smc";
	};

	extal_clk: extal {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		/* This value must be overridden by the board */
		clock-frequency = <0>;
	};

	extalr_clk: extalr {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		/* This value must be overridden by the board */
		clock-frequency = <0>;
	};

	/* MSIOF reference clock - to be overridden by boards that provide it */
	msiof_ref_clk: msiof-ref-clock {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <0>;
	};

	ref_clk: refclk {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		/* This value must be overridden by the board */
		clock-frequency = <0>;
	};

	/* External PCIe clock - can be overridden by the board */
	pcie_bus_clk: pcie_bus {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <0>;
	};

	/* External SCIF clock - to be overridden by boards that provide it */
	scif_clk: scif {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <0>;
	};

	timer {
		compatible = "arm,armv8-timer";
		interrupts-extended = <&gic GIC_PPI 13 (GIC_CPU_MASK_SIMPLE(8) | IRQ_TYPE_LEVEL_LOW) 0>,
				      <&gic GIC_PPI 14 (GIC_CPU_MASK_SIMPLE(8) | IRQ_TYPE_LEVEL_LOW) 0>,
				      <&gic GIC_PPI 11 (GIC_CPU_MASK_SIMPLE(8) | IRQ_TYPE_LEVEL_LOW) 0>,
				      <&gic GIC_PPI 10 (GIC_CPU_MASK_SIMPLE(8) | IRQ_TYPE_LEVEL_LOW) 0>;
	};

	pmu_a76 {
		compatible = "arm,cortex-a76-pmu";
		interrupts-extended = <&gic GIC_PPI 7 IRQ_TYPE_LEVEL_HIGH 0>;
	};

	soc {
		compatible = "simple-bus";
		interrupt-parent = <&gic>;

		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		cpg: clock-controller@e6150000 {
			compatible = "renesas,r8a779a0-cpg-mssr";
			reg = <0 0xe6150000 0 0x10000>;
			clocks = <&extal_clk>, <&extalr_clk>;
			clock-names = "extal", "extalr";
			#clock-cells = <2>;
			#power-domain-cells = <0>;
			#reset-cells = <1>;
		};

		rst: reset-controller@e6160000 {
			compatible = "renesas,r8a779a0-rst";
			reg = <0 0xe6160000 0 0x0200>;
		};

		gic: interrupt-controller@f1000000 {
			compatible = "arm,gic-v3";

			#interrupt-cells = <4>;
			#address-cells = <0>;
			interrupt-controller;
			reg = <0x0 0xf1000000 0x0 0x20000>,
			      <0x0 0xf1060000 0x0 0x110000>;
			interrupts = <GIC_PPI 9	(GIC_CPU_MASK_SIMPLE(8) |
				      IRQ_TYPE_LEVEL_HIGH) 0>;
			power-domains = <&sysc R8A779A0_PD_ALWAYS_ON>;

			ppi-partitions {
				ppi_cluster0: interrupt-partition-0 {
					affinity = <&a76_0 &a76_1>;
				};
				ppi_cluster1: interrupt-partition-1 {
					affinity = <&a76_2 &a76_3>;
				};
				ppi_cluster2: interrupt-partition-2 {
					affinity = <&a76_4 &a76_5>;
				};
				ppi_cluster3: interrupt-partition-3 {
					affinity = <&a76_6 &a76_7>;
				};
			};
		};

		rwdt: watchdog@e6020000 {
			compatible = "renesas,r8a779a0-wdt",
				     "renesas,rcar-gen3-wdt";
			reg = <0 0xe6020000 0 0x0c>;
			clocks = <&cpg CPG_MOD 907>;
			power-domains = <&sysc R8A779A0_PD_ALWAYS_ON>;
			resets = <&cpg 907>;
			status = "disabled";
		};

		gpio1: gpio@e6050180 {
			compatible = "renesas,gpio-r8a779a0";
			reg = <0 0xe6050180 0 0x54>;
			interrupts = <GIC_SPI 836 IRQ_TYPE_LEVEL_HIGH 0>;
			#gpio-cells = <2>;
			gpio-controller;
			gpio-ranges = <&pfc 0 32 31>;
			#interrupt-cells = <2>;
			interrupt-controller;
			clocks = <&cpg CPG_MOD 915>;
			power-domains = <&sysc R8A779A0_PD_ALWAYS_ON>;
			resets = <&cpg 1330>;
			status = "disabled";
		};

		gpio2: gpio@e6050980 {
