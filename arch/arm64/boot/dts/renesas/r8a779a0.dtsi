			interrupts = <GIC_SPI 856 IRQ_TYPE_LEVEL_HIGH 0>;
			#gpio-cells = <2>;
			gpio-controller;
			gpio-ranges = <&pfc 0 192 21>;
			#interrupt-cells = <2>;
			interrupt-controller;
			clocks = <&cpg CPG_MOD 918>;
			power-domains = <&sysc R8A779A0_PD_ALWAYS_ON>;
			resets = <&cpg 1401>;
			status = "disabled";
		};

		gpio7: gpio@e6068980 {
			compatible = "renesas,gpio-r8a779a0";
