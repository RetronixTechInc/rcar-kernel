			interrupts = <GIC_SPI 852 IRQ_TYPE_LEVEL_HIGH 0>;
			#gpio-cells = <2>;
			gpio-controller;
			gpio-ranges = <&pfc 0 160 21>;
			#interrupt-cells = <2>;
			interrupt-controller;
			clocks = <&cpg CPG_MOD 917>;
			power-domains = <&sysc R8A779A0_PD_ALWAYS_ON>;
			resets = <&cpg 1400>;
			status = "disabled";
		};

		gpio6: gpio@e6068180 {
